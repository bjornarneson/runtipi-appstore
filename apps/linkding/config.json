{
  "$schema": "../app-info-schema.json",
  "name": "linkding",
  "available": true,
  "port": 9090,
  "exposable": true,
  "dynamic_config": true,
  "id": "linkding",
  "description": "Self-hosted bookmark manager that is designed be to be minimal, fast, and easy to set up using Docker.",
  "tipi_version": 5,
  "version": "1.44.1",
  "categories": ["utilities"],
  "short_desc": "Self-hosted bookmark manager.",
  "author": "sissbruecker",
  "source": "https://github.com/sissbruecker/linkding",
  "website": "https://linkding.link/",
  "form_fields": [
    {
      "type": "text",
      "label": "Username of the initial superuser",
      "max": 50,
      "min": 3,
      "required": true,
      "env_variable": "LD_SUPERUSER_NAME"
    },
    {
      "type": "password",
      "label": "Password for the initial superuser",
      "max": 50,
      "min": 8,
      "required": true,
      "env_variable": "LD_SUPERUSER_PASSWORD"
    }
  ],  
  "supported_architectures": ["arm64", "amd64"]
}
