{
  "services": [
    {
      "name": "${LD_CONTAINER_NAME:-linkding}",
      "image": "sissbruecker/linkding:latest",
      "internalPort": 9090,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data}",
          "containerPath": "/etc/linkding/data",
          "readOnly": false,
          "shared": false,
          "private": false
        }
      ],
      "isMain": true
    }
  ]
}
